<html>
    <head>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
        <style>
            .steamicon
            {
                width: 32px;
                height: 32px;
                display: inline-block;
            }
        </style>
    </head>
    <body>
        <div class="container">
            <h1>
                steam-match-web
            </h1>
            {% for game in l %}
                <p>
                    {% if game.icon is equalto "" %}
                        <img class="steamicon" src=""> {{ game.name }}
                    {% else %}
                        <img class="steamicon" src="http://media.steampowered.com/steamcommunity/public/images/apps/{{ game.appid }}/{{ game.icon }}.jpg"> {{ game.name }}
                    {% endif %}
                </p>
            {% endfor %}
        </div>
    </body>
</html>
